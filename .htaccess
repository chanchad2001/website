RewriteEngine On 
#accueil
RewriteRule ^accueil$ /throows-dev/index.php?page=accueil
#RewriteRule ^accueil$ /index.php?page=accueil

#profil
RewriteRule ^profil/(.*) /throows-dev/index.php?page=profil&profile=$1
#RewriteRule ^profil/(.*) /index.php?page=profil&profile=$1

#tuto 
RewriteRule ^tutoriel$ /throows-dev/index.php?page=tutoriel
#RewriteRule ^tutoriel$ /index.php?page=tutoriel

#boutique
RewriteRule ^boutique$ /throows-dev/index.php?page=boutique
#RewriteRule ^boutique$ index.php?page=boutique

#blog
RewriteRule ^blog/web$ /throows-dev/index.php?page=blog&theme=web
RewriteRule ^blog/mc-dev$ /throows-dev/index.php?page=blog&theme=mc-dev
RewriteRule ^blog/game-engine-dev$ /throows-dev/index.php?page=blog&theme=game-engine-dev
