#RewriteRule ^blog/web$ /index.php?page=blog&theme=web
#RewriteRule ^blog/mc-dev$ /index.php?page=blog&theme=mc-dev
#RewriteRule ^blog/game-engine-dev$ /index.php?page=blog&theme=game-engine-dev
#RewriteRule ^blog$ /index.php?page=blog

#contact
RewriteRule ^contact$ /throows-dev/index.php?page=contact
#RewriteRule ^contact$ index.php?page=contact

#login
RewriteRule ^login/returnto=(.*) /throows-dev/index.php?page=login&source=$1
